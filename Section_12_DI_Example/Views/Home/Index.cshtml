@model IEnumerable<string>
@inject ICitiesServices citiesServices;

@{
    ViewBag.Title = "Home";
}

@{
   List<string> cities =  citiesServices.GetCities();
}

<h2>Cities</h2>

<ul class="list">
    @foreach (string city in cities)
    {
        <li>@city</li>
    }

</ul>


<div>@ViewBag.Instance1</div>
<div>@ViewBag.Instance2</div>
<div>@ViewBag.Instance3</div>
<div>@citiesServices.InstanceId</div>

<div class="box">
    @ViewBag.scope_InstanceID
</div>